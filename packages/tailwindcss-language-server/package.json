{
  "name": "@tailwindcss/language-server",
  "description": "Tailwind CSS Language Server",
  "license": "MIT",
  "version": "0.0.11",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tailwindlabs/tailwindcss-intellisense.git",
    "directory": "packages/tailwindcss-language-server"
  },
  "bugs": {
    "url": "https://github.com/tailwindlabs/tailwindcss-intellisense/issues"
  },
  "homepage": "https://github.com/tailwindlabs/tailwindcss-intellisense/tree/HEAD/packages/tailwindcss-language-server#readme",
  "scripts": {
    "build": "npm run clean && npm run _esbuild && npm run hashbang",
    "_esbuild": "node ../../esbuild.js src/server.ts --outfile=bin/tailwindcss-language-server --external=pnpapi --minify",
    "clean": "rimraf bin",
    "hashbang": "node scripts/hashbang.mjs",
    "create-notices-file": "node scripts/createNoticesFile.mjs",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "tailwindcss-language-server": "./bin/tailwindcss-language-server"
  },
  "files": [
    "bin",
    "ThirdPartyNotices.txt"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@parcel/watcher": "2.0.3",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.7",
    "@types/debounce": "1.2.0",
    "@types/node": "14.14.34",
    "@types/vscode": "1.65.0",
    "builtin-modules": "3.2.0",
    "chokidar": "3.5.1",
    "color-name": "1.1.4",
    "culori": "0.20.1",
    "debounce": "1.2.0",
    "deepmerge": "4.2.2",
    "detective": "5.2.0",
    "dlv": "1.1.3",
    "dset": "3.1.2",
    "enhanced-resolve-301": "0.0.1",
    "fast-glob": "3.2.4",
    "find-up": "5.0.0",
    "jest": "25.5.4",
    "klona": "2.0.4",
    "license-checker": "25.0.1",
    "minimatch": "5.1.4",
    "normalize-path": "3.0.0",
    "pkg-up": "3.1.0",
    "postcss": "8.3.9",
    "postcss-load-config": "3.0.1",
    "postcss-selector-parser": "6.0.2",
    "prettier": "2.3.0",
    "resolve": "1.20.0",
    "rimraf": "3.0.2",
    "stack-trace": "0.0.10",
    "tailwindcss": "3.2.4",
    "typescript": "4.6.4",
    "vscode-css-languageservice": "5.4.1",
    "vscode-languageserver": "8.0.2",
    "vscode-languageserver-textdocument": "1.0.7",
    "vscode-uri": "3.0.2"
  }
}
